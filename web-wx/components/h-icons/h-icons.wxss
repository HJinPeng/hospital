/* components/h-icons/h-icons.wxss */
.content {
  display: flex;
  width: 100%;
  flex-wrap: wrap;
  /* justify-content: center; */
  align-items: center;
  text-align: center;
}
.content .item {
  width: 25%;
  padding: 14rpx 30rpx;
}
.content .item .image {
  display: block;
  width: 80%;
  margin: auto;
  border: 1px solid #acd9f2;
  border-radius: 50%;
  background-color: #fff;
}
.content .item .name {
  font-size: 24rpx;
  margin-top: 10rpx;
  color: #1296db;
}